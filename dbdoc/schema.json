    {
      "name": "public.node_stats",
      "type": "VIEW",
      "comment": "Aggregated statistics by node type showing counts, importance, tokens, and age ranges.",
      "columns": [
        {
          "name": "type",
          "type": "text",
          "nullable": true
        },
        {
          "name": "count",
          "type": "bigint",
          "nullable": true
        },
        {
          "name": "avg_importance",
          "type": "double precision",
          "nullable": true
        },
        {
          "name": "total_tokens",
          "type": "bigint",
          "nullable": true
        },
        {
          "name": "oldest",
          "type": "timestamp with time zone",
          "nullable": true
        },
        {
          "name": "newest",
          "type": "timestamp with time zone",
          "nullable": true
        }
      ],
      "def": "CREATE VIEW node_stats AS (\n SELECT type,\n    count(*) AS count,\n    avg(importance) AS avg_importance,\n    sum(token_count) AS total_tokens,\n    min(created_at) AS oldest,\n    max(created_at) AS newest\n   FROM nodes\n  GROUP BY type\n)",
      "referenced_tables": [
        "public.nodes"
      ]
    },
    {
      "name": "public.robot_activity",
      "type": "VIEW",
      "comment": "Robot usage metrics showing total nodes created and last activity timestamp.",
      "columns": [
        {
          "name": "id",
          "type": "text",
          "nullable": true
        },
        {
          "name": "name",
          "type": "text",
          "nullable": true
        },
        {
          "name": "total_nodes",
          "type": "bigint",
          "nullable": true
        },
        {
          "name": "last_node_created",
          "type": "timestamp with time zone",
          "nullable": true
        }
      ],
      "def": "CREATE VIEW robot_activity AS (\n SELECT r.id,\n    r.name,\n    count(n.id) AS total_nodes,\n    max(n.created_at) AS last_node_created\n   FROM (robots r\n     LEFT JOIN nodes n ON ((n.robot_id = r.id)))\n  GROUP BY r.id, r.name\n)",
      "referenced_tables": [
        "public.robots",
        "public.nodes"
      ]
    },
    {
      "name": "public.ontology_structure",
      "type": "VIEW",
      "comment": "Provides a hierarchical view of all topics in the knowledge base. Topics use colon-delimited format (e.g., database:postgresql:timescaledb) and are assigned manually via tags.",
      "columns": [
        {
          "name": "root_topic",
          "type": "text",
          "nullable": true
        },
        {
          "name": "level1_topic",
          "type": "text",
          "nullable": true
        },
        {
          "name": "level2_topic",
          "type": "text",
          "nullable": true
        },
        {
          "name": "full_path",
          "type": "text",
          "nullable": true
        },
        {
          "name": "node_count",
          "type": "bigint",
          "nullable": true
        }
      ],
      "def": "CREATE VIEW ontology_structure AS (\n SELECT split_part(tag, ':'::text, 1) AS root_topic,\n    split_part(tag, ':'::text, 2) AS level1_topic,\n    split_part(tag, ':'::text, 3) AS level2_topic,\n    tag AS full_path,\n    count(DISTINCT node_id) AS node_count\n   FROM tags\n  WHERE (tag ~ '^[a-z0-9\\-]+(:[a-z0-9\\-]+)*$'::text)\n  GROUP BY tag\n  ORDER BY (split_part(tag, ':'::text, 1)), (split_part(tag, ':'::text, 2)), (split_part(tag, ':'::text, 3))\n)",
      "referenced_tables": [
        "public.tags"
      ]
    },
    {
      "name": "public.topic_relationships",
      "type": "VIEW",
      "comment": "Shows which topics co-occur on the same nodes, revealing cross-topic relationships in the knowledge base.",
      "columns": [
        {
          "name": "topic1",
          "type": "text",
          "nullable": true
        },
        {
          "name": "topic2",
          "type": "text",
          "nullable": true
        },
        {
          "name": "shared_nodes",
          "type": "bigint",
          "nullable": true
        }
      ],
      "def": "CREATE VIEW topic_relationships AS (\n SELECT t1.tag AS topic1,\n    t2.tag AS topic2,\n    count(DISTINCT t1.node_id) AS shared_nodes\n   FROM (tags t1\n     JOIN tags t2 ON (((t1.node_id = t2.node_id) AND (t1.tag \u003c t2.tag))))\n  GROUP BY t1.tag, t2.tag\n HAVING (count(DISTINCT t1.node_id) \u003e= 2)\n  ORDER BY (count(DISTINCT t1.node_id)) DESC\n)",
      "referenced_tables": [
        "public.tags"
      ]
    }
  ],
